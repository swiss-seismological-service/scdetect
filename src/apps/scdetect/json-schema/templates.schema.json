{
  "type": "array",
  "minItems": 1,
  "items": {
    "title": "Detector Configuration",
    "type": "object",
    "properties": {
      "arrivalOffsetThreshold": {
        "type": "number"
      },
      "createArrivals": {
        "type": "boolean"
      },
      "createTemplateArrivals": {
        "type": "boolean"
      },
      "detectorId": {
        "type": "string"
      },
      "gapInterpolation": {
        "type": "boolean"
      },
      "gapThreshold": {
        "type": "number",
        "minimum": 0
      },
      "gapTolerance": {
        "type": "number",
        "minimum": 0
      },
      "maximumLatency": {
        "type": "number",
        "minimum": 0
      },
      "minimumArrivals": {
        "type": "integer",
        "minimum": 1
      },
      "originId": {
        "type": "string"
      },
      "streams": {
        "type": "array", 
        "minItems": 1,
        "items": {
          "type": "object",
          "title": "Stream Configuration",
          "properties": {
            "filter": {
              "type": "string"
            },
            "initTime": {
              "type": "number",
              "minimum": 0
            },
            "templateFilter": {
              "type": "string"
            },
            "templateId": {
              "type": "string"
            },
            "templatePhase": {
              "type": "string"
            },
            "templateWaveformId": {
              "type": "string"
            },
            "templateWaveformStart": {
              "type": "number"
            },
            "templateWaveformEnd": {
              "type": "number"
            }
          },
          "required": ["templatePhase", "waveformId"]
        }
      }
    },
    "timeCorrection": {
      "type": "number"
    },
    "triggerDuration": {
      "type": "number"
    },
    "triggerOnThreshold": {
      "type": "number",
      "minimum": -1,
      "maximum": 1
    },
    "triggerOffThreshold": {
      "type": "number",
      "minimum": -1,
      "maximum": 1
    },
    "required": ["originId", "streams"]
  }
}
